{
  "name": "bau-cms-cli",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/bau-cms-cli/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "../../dist/packages/bau-cms-cli",
        "main": "packages/bau-cms-cli/src/index.ts",
        "tsConfig": "packages/bau-cms-cli/tsconfig.json",
        "assets": ["packages/bau-cms-cli/*.md"],
        "platform": "node",
        "format": ["cjs"],
        "bundle": true,
        "thirdParty": true,
        "generatePackageJson": false,
        "esbuildOptions": {
          "outExtension": {
            ".js": ".js"
          }
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/bau-cms-cli/**/*.ts"]
      }
    }
  },
  "tags": []
}
