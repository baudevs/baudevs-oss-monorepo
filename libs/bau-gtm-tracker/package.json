{
  "name": "@baudevs/bau-gtm-tracker",
  "version": "1.0.16",
  "description": "A robust library to intercept Fetch, XHR, and WebSocket requests and push events to dataLayer for GTM",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "analytics",
    "tracking",
    "xhr",
    "fetch",
    "websocket"
  ],
  "author": "Baudevs",
  "license": "CC BY-NC 4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/baudevs/baudevs-monorepo.git/libs/bau-gtm-tracker"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "unpkg": "./index.umd.js",
  "jsdelivr": "./index.umd.js",
  "files": [
    "*.js",
    "*.cjs.js",
    "*.mjs.js",
    "*.umd.js",
    "*.umd.min.js",
    "*.umd.min.gz",
    "*.umd.min.js.map",
    "*.esm.js",
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.cjs.js",
    "src/**/*.mjs.js",
    "src/**/*.umd.js",
    "src/**/*.esm.js",
    "LICENSE",
    "*.md"
  ],
  "sideEffects": false,
  "dependencies": {
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-gzip": "^3.1.2",
    "@nx/rollup": "20.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "typescript": "^5.7.2",
    "@nx/rollup": "^20.3.0",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-gzip": "^3.1.2"
  }
}
