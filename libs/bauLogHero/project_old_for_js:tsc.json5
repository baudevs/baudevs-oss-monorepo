{
  "name": "@baudevs/bau-log-hero",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/bauLogHero/src",
  "projectType": "library",
  "tags": [],
  "type": "module",
  "targets": {
    "test": {
      "cache": false,
      "executor": "@nx/jest:jest",
      "dependsOn": ["^build"],
      "inputs": ["default", "production", "^production"],
      "outputs": [],
      "options": {
        "jestConfig": "libs/bauLogHero/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build": {
      "cache": false,
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/@baudevs/bau-log-hero",
        "main": "libs/bauLogHero/src/index.ts",
        "tsConfig": "libs/bauLogHero/tsconfig.lib.json",
        "assets": [
          "libs/bauLogHero/*.md",
          "libs/bauLogHero/package.json"
        ]
      },
      "configurations": {
        "esm": {
          "tsConfig": "libs/bauLogHero/tsconfig.lib.esm.json",
          "outputPath": "dist/libs/@baudevs/bau-log-hero/esm",
          "main": "libs/bauLogHero/src/index.ts"
        },
        "cjs": {
          "tsConfig": "libs/bauLogHero/tsconfig.lib.cjs.json",
          "outputPath": "dist/libs/@baudevs/bau-log-hero/cjs",
          "main": "libs/bauLogHero/src/index.ts"
        }
      }
    },
    "release": {
      "cache": false,
      "options": {
        "project": "@baudevs/bau-log-hero",
        "packageRoot": "dist/libs/@baudevs/bau-log-hero",
        "publish": false
      },
      "changelog": {
        "projectChangelogs": {
          "file": "CHANGELOG.md",
          "createRelease": "github"
        }
      }
    },
    "publish": {
      "options": {
        "project": "@baudevs/bau-log-hero",
        "packageRoot": "dist/libs/@baudevs/bau-log-hero"
      }
    }
  }
}

