{
  "name": "@baudevs/bau-log-hero",
  "version": "1.3.0",
  "description": "A simple, no dependencies logging library for browser and node, with colors, timestamp, and easy to activate/deactivate by file and compatible with tree-shakeable",
  "keywords": [
    "logging",
    "browser",
    "node",
    "colors",
    "timestamp",
    "baudevs",
    "baudevs-monorepo"
  ],
  "author": "Baudevs",
  "license": "CC BY-NC 4.0",
  "private": false,
  "projectType": "library",
  "repository": {
    "type": "git",
    "url": "https://github.com/baudevs/baudevs-monorepo.git/bauLogHero"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../dist/libs/@baudevs/bau-log-hero",
    "pwd": "exec -- pwd"
  },
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "unpkg": "./umd/index.js",
  "jsdelivr": "./umd/index.js.gz",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    }
  },
  "files": [
    "cjs",
    "esm",
    "umd",
    "types",
    "LICENSE",
    "*.md"
  ],
  "dependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^10.0.1",
    "autoprefixer": "^10.4.20",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-gzip": "^3.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/node": "^20.17.12",
    "rimraf": "^5.0.10",
    "typescript": "^5.7.2"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  }
}
