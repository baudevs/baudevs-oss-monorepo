{
  "name": "@baudevs/bau-log-hero",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/bauLogHero/src",
  "projectType": "library",
  "type": "module",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bauLogHero/**/*.ts"]
      }
    },
    "build": {
      "cache": false,
      "dependsOn": ["lint"],
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @baudevs/bau-log-hero run build",
        "parallel": false
      }
    },
    "release": {
      "cache": false,
      "options": {
        "project": "@baudevs/bau-log-hero",
        "packageRoot": "dist/libs/@baudevs/bau-log-hero",
        "publish": false
      },
      "changelog": {
        "projectChangelogs": {
          "file": "CHANGELOG.md",
          "createRelease": "github"
        }
      }
    },
    "publish": {
      "options": {
        "project": "@baudevs/bau-log-hero",
        "packageRoot": "dist/libs/@baudevs/bau-log-hero"
      }
    },
    "printhello": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "echo hello"
      }
    }
  }
}
